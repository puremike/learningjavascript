let counter = 1;
while (counter <= 7) {
  console.log(`WHILE: Lifting Weights! ${counter}`);
  counter++;
}

//ADD THE FIRST 10 NUMBERS
let genNumber = 1;
let sum = 0;
while (genNumber <= 10) {
  sum += genNumber;
  genNumber++;
}
console.log(sum);

let dice = Math.trunc(Math.random() * 6) + 1;
while (dice !== 6) {
  console.log(`You rolled a ${dice}`);
  dice = Math.trunc(Math.random() * 6) + 1;
}
